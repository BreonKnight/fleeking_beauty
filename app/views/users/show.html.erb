<div class="container prof">
	<div class="row">
		<div class="col-md-12">
			  <h2 class="text-center">Welcome, <%= @user.userName %> </h2>
			  <%= image_tag @user.avatar.url, size: "320x426", class: "img-responsive" %>
			  <h3><%=@user.first_name%> <%=@user.last_name%></h3>
			  <% if current_user == @user %>
			  <%= button_to "Edit", edit_user_path, method: :get, class: "btn btn-default" %>

				<%= button_to "Delete", @user, method: :delete, class: "btn btn-danger" %>
				<% end %>
		</div>
	</div>
</div>
<div class ="container prof">
	<div class = "row">
		<svg id="graph"></svg>
	<br>
	Total Upvotes: <%= @user.upvotes.count  %> 
	<br>
	Total Downvotes: <%= @user.downvotes.count  %> 
	<br>
	<%= @upvotes.count %>
	<br>
	<%= @upvotes.each do |place| %>
		
	<% end %>
